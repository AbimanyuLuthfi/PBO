public class ClassAttributes {
    int x = 5;
    int y = 3;
    int z = 10;
    final int o = 10;

    public static void main(String[] args){
        ClassAttributes myObj1 = new ClassAttributes(); //Object 1 baru
        System.out.println(myObj1.x); // Outputnya 5
        System.out.println(myObj1.y); // Outputnya 3

        ClassAttributes myObj2 = new ClassAttributes(); //Object 2 baru
        myObj2.x = 50;
        System.out.println(myObj2.x); // Nilai x pada Object 2 berubah menjadi 50

        ClassAttributes myObj3 = new ClassAttributes(); //Object 3 baru
        myObj3.x = 40;
        System.out.println(myObj3.x); // Nilai z pada Object 3 berubah menjadi 40

        /*
        ClassAttributes myObj4 = new ClassAttributes(); //Object 4 baru
        myObj4.o = 25;
        System.out.println(myObj4.x); // Nilai o pada object 4 akan error karena nilai pada final int tidak bisa diubah
        */

        ClassAttributes myObj5 = new ClassAttributes(); //Object 5 baru
        myObj5.x = 25;
        System.out.println(myObj1.x);
        System.out.println(myObj5.x); 

    }

}
